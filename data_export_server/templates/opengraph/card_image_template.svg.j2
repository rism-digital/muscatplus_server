    <text style="fill: rgb(0, 80, 145); font-family: 'Noto Sans Display'; font-size: 47px; font-weight: 700; white-space: pre;"
          x="36.3" y="546.481">RISM Online
    </text>
    <!-- /rism online wordmark -->

    <!-- main card title -->
    <g transform="matrix(1, 0, 0, 1, -8.022994, 6.486227)">
        {% for linetext in record_title %}
            {% set lineoffset = "86.485" if loop.first else "150.675" %}
            <text style="fill: rgb(51, 51, 51); font-family: monospace; font-size: 45px; font-weight: 700; white-space: pre;"
                      x="198.023" y="{{ lineoffset }}">{{ linetext|e }}
            </text>
        {% endfor %}
    </g>
    <!-- /main card title -->

    <!-- top icon block -->
    <g transform="translate(40, 50)">
        <rect width="120px" height="120px" class="{{ record_type }}" />
    </g>

    <!-- /icon block -->

    <!-- first line -->
    {% if record_first_line %}
    {% set icon, label = record_first_line %}
    <g transform="translate(40, 210)" id="field1">
        <rect width="50px" height="50px" class="{{ icon }}"/>
    </g>
    <text style="fill: rgb(51, 51, 51); font-family: monospace; font-size: 32px; font-weight: 700; white-space: pre;"
          x="105.434" y="244">{{ label|e }}
    </text>
    {% endif %}
    <!-- /first line -->

    <!-- second line -->
    {% if record_second_line %}
    {% set icon, label = record_second_line %}
    <g transform="translate(40, 280)" id="field1">
        <rect width="50px" height="50px" class="{{ icon }}"/>
    </g>
    <text style="fill: rgb(51, 51, 51); font-family: monospace; font-size: 32px; font-weight: 700; white-space: pre;"
          x="105.434" y="314">{{ label|e }}
    </text>

    {% endif %}
    <!-- /second line -->

    <!-- third line -->
    {% if record_third_line %}
        {% set icon, label = record_third_line %}
        <g transform="translate(40, 350)" id="field1">
            <rect width="50px" height="50px" class="{{ icon }}"/>
        </g>
        <text style="fill: rgb(51, 51, 51); font-family: monospace; font-size: 32px; font-weight: 700; white-space: pre;"
              x="105.434" y="382">{{ label|e }}
        </text>

    {% endif %}
    <!-- /third line -->
    <rect x="0.187" y="186.334" width="1201" height="4.077" style="stroke: rgb(0, 4, 240); fill: rgb(83, 132, 248); stroke-width: 2px; fill-opacity: 0.47; stroke-opacity: 0.12;"/>
</svg>